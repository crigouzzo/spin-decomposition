Cell["\<\
The following lines allow to view the components of matrices in a nicer way:\
\>", "Text",
 CellChangeTimes->{{3.897385088108929*^9, 
  3.897385135807791*^9}},ExpressionUUID->"84ea7cbb-630c-4b74-a96c-\
8e08387758b5"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ShowVector", "[", "v_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "p", "}"}], ",", 
     RowBox[{
      RowBox[{"p", "=", 
       RowBox[{"Apply", "[", 
        RowBox[{"PolynomialGCD", ",", "v"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"ArrayQ", "[", "p", "]"}], ",", 
        RowBox[{"p", "=", 
         RowBox[{"p", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ",", "p"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Row", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"ContainsExactly", "[", 
            RowBox[{
             RowBox[{"{", "p", "}"}], ",", 
             RowBox[{"{", "1", "}"}]}], "]"}], ",", "Null", ",", "p"}], "]"}],
          ",", " ", 
         RowBox[{"MatrixForm", "[", 
          RowBox[{
           RowBox[{"v", "/", "p"}], "//", "Simplify"}], "]"}]}], "}"}], 
       "]"}]}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Factoras", "[", 
    RowBox[{"v_", ",", "w_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", 
     RowBox[{"Row", "[", 
      RowBox[{"{", 
       RowBox[{"w", ",", " ", 
        RowBox[{"MatrixForm", "[", 
         RowBox[{
          RowBox[{"v", "/", "w"}], "//", "FullSimplify"}], "]"}]}], "}"}], 
      "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Factoras", "[", 
    RowBox[{"v_", ",", "w_", ",", "x_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", 
     RowBox[{"Row", "[", 
      RowBox[{"{", 
       RowBox[{"x", ",", " ", 
        RowBox[{"MatrixForm", "[", 
         RowBox[{
          RowBox[{"v", "/", "w"}], "//", "FullSimplify"}], "]"}]}], "}"}], 
      "]"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"$PrePrint1", "=", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"ArrayQ", "[", "#", "]"}], ",", 
      RowBox[{"MatrixForm", "[", "#", "]"}], ",", "#"}], "]"}], "&"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"$PrePrint2", "=", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"ArrayQ", "[", "#", "]"}], ",", 
      RowBox[{"Quiet", "[", 
       RowBox[{"Check", "[", 
        RowBox[{
         RowBox[{"ShowVector", "[", "#", "]"}], ",", 
         RowBox[{"MatrixForm", "[", "#", "]"}]}], "]"}], "]"}], ",", "#"}], 
     "]"}], "&"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"$PrePrint", "=", "$PrePrint1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Unprotect", "[", "Conjugate", "]"}], ";", 
  RowBox[{"Conjugate", "/:", 
   RowBox[{"MakeBoxes", "[", 
    RowBox[{
     RowBox[{"Conjugate", "[", "x_", "]"}], ",", "StandardForm"}], "]"}], ":=", 
   RowBox[{"TemplateBox", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"Parenthesize", "[", 
       RowBox[{"x", ",", "StandardForm", ",", "Power"}], "]"}], "}"}], ",", 
     "\"\<Conjugate\>\"", ",", 
     RowBox[{"DisplayFunction", "\[Rule]", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"SuperscriptBox", "[", 
         RowBox[{"#1", ",", "\"\<*\>\""}], "]"}], "&"}], ")"}]}]}], 
    "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{"Protect", "[", "Conjugate", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"MakeBoxes", "[", 
   RowBox[{
    RowBox[{"Abs", "[", "x_", "]"}], ",", "StandardForm"}], "]"}], ":=", 
  RowBox[{"MakeBoxes", "@", 
   RowBox[{"BracketingBar", "[", "x", "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{"BracketingBar", "=", "Abs"}], ";"}]}], "Input",
