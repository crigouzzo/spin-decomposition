Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ShowVector", "[", "v_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "p", "}"}], ",", 
     RowBox[{
      RowBox[{"p", "=", 
       RowBox[{"Apply", "[", 
        RowBox[{"PolynomialGCD", ",", "v"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"ArrayQ", "[", "p", "]"}], ",", 
        RowBox[{"p", "=", 
         RowBox[{"p", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ",", "p"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Row", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"ContainsExactly", "[", 
            RowBox[{
             RowBox[{"{", "p", "}"}], ",", 
             RowBox[{"{", "1", "}"}]}], "]"}], ",", "Null", ",", "p"}], "]"}],
          ",", " ", 
         RowBox[{"MatrixForm", "[", 
          RowBox[{
           RowBox[{"v", "/", "p"}], "//", "Simplify"}], "]"}]}], "}"}], 
       "]"}]}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Factoras", "[", 
    RowBox[{"v_", ",", "w_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", 
     RowBox[{"Row", "[", 
      RowBox[{"{", 
       RowBox[{"w", ",", " ", 
        RowBox[{"MatrixForm", "[", 
         RowBox[{
          RowBox[{"v", "/", "w"}], "//", "FullSimplify"}], "]"}]}], "}"}], 
      "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Factoras", "[", 
    RowBox[{"v_", ",", "w_", ",", "x_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", 
     RowBox[{"Row", "[", 
      RowBox[{"{", 
       RowBox[{"x", ",", " ", 
        RowBox[{"MatrixForm", "[", 
         RowBox[{
          RowBox[{"v", "/", "w"}], "//", "FullSimplify"}], "]"}]}], "}"}], 
      "]"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"$PrePrint1", "=", 
   RowBox[{
    RowBox[{"If", "[", 
