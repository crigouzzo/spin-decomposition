Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ShowVector", "[", "v_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "p", "}"}], ",", 
     RowBox[{
      RowBox[{"p", "=", 
